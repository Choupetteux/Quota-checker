<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.91" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quota.QuotaController">
   <children>
      <StackPane prefHeight="75.0" prefWidth="200.0">
         <children>
            <Button fx:id="runButton" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#onRun" text="Lancer" StackPane.alignment="BOTTOM_CENTER">
               <StackPane.margin>
                  <Insets bottom="10.0" />
               </StackPane.margin>
            </Button>
            <ProgressBar fx:id="progressBar" maxWidth="1.7976931348623157E308" prefWidth="200.0" progress="0.0" StackPane.alignment="TOP_CENTER">
               <StackPane.margin>
                  <Insets top="15.0" />
               </StackPane.margin>
            </ProgressBar>
            <Label fx:id="progressLabel" StackPane.alignment="TOP_CENTER">
               <StackPane.margin>
                  <Insets top="15.0" />
               </StackPane.margin>
            </Label>
         </children>
      </StackPane>
   </children>
</VBox>
